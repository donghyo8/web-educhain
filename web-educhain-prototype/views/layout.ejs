        <!-- Modal -->
        <div id="ModalLogin" class="modal fade">
            <div class="modal-dialog modal-login">
                <div class="modal-content">
                    <div class="modal-header">
                        <h4 class="modal-title">로그인</h4>
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    </div>
                    <div class="modal-body">
                        <form name="login">
                            <div class="form-group">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-pencil"></i></span>
                                    <input type="text" class="form-control" id="input_ID" name="userID" placeholder="ID"
                                        required="required">
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-lock"></i></span>
                                    <input type="password" class="form-control" id="input_PW" name="userPW"
                                        placeholder="Password" required="required">
                                </div>
                            </div>
                            <div class="form-group">
                                <button type="button" id="login_btn"
                                    class="btn btn-primary btn-block btn-lg">로그인</button>
                            </div>
                            <script>
                                $("#login_btn").click(function () {
                                    var userID = $("input[name=userID]").val();
                                    var userPW = $("input[name=userPW]").val();
                                    if (userID != "" && userPW != "") {
                                        $.post('/login', { id: userID, pw: userPW }, function (data) {
                                            if (data.msg != null) {
                                                alert(data.msg);
                                            } else {
                                                window.location.reload();
                                            }
                                        })
                                    }
                                    else {
                                        alert("아이디 또는 패스워드를 입력하세요!");
                                    }
                                });
                                $("#input_ID").keydown(function (e) {
                                    if (e.keyCode == 13) {
                                        var userID = $("input[name=userID]").val();
                                        var userPW = $("input[name=userPW]").val();
                                        if (userID != "" && userPW != "") {
                                            $.post('/login', { id: userID, pw: userPW }, function (data) {
                                                if (data.msg != null) {
                                                    alert(data.msg);
                                                } else {
                                                }
                                            })
                                        }
                                        else {
                                            alert("아이디 또는 패스워드를 입력하세요!");
                                        }
                                    }
                                })
                                $("#input_PW").keydown(function (e) {
                                    if (e.keyCode == 13) {
                                        var userID = $("input[name=userID]").val();
                                        var userPW = $("input[name=userPW]").val();
                                        if (userID != "" && userPW != "") {
                                            $.post('/login', { id: userID, pw: userPW }, function (data) {
                                                if (data.msg != null) {
                                                    alert(data.msg);
                                                } else {
                                                    window.location.reload();
                                                }
                                            })
                                        }
                                        else {
                                            alert("아이디 또는 패스워드를 입력하세요!");
                                        }
                                    }
                                })
                            </script>
                        </form>
                    </div>
                    <div class="modal-footer"></div>
                </div>
            </div>
        </div>
        <div id="ModalJoin" class="modal fade">
            <div class="modal-dialog modal-login">
                <div class="modal-content">
                    <div class="modal-header">
                        <h4 class="modal-title">회원가입</h4>
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    </div>
                    <div class="modal-body">
                        <form action="/join" method="post">
                            <div class="form-group">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-pencil"></i></span>
                                    <input type="text" class="form-control" name="new_userID" placeholder="ID"
                                        required="required">
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-lock"></i></span>
                                    <input type="password" class="form-control" name="new_userPW" placeholder="Password"
                                        required="required">
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-lock"></i></span>
                                    <input type="password" class="form-control" name="new_userPW_confirm"
                                        placeholder="Password-Confirm" required="required">
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-user"></i></span>
                                    <input type="text" class="form-control" name="new_userName" placeholder="User Name"
                                        required="required">
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-lock"></i></span>
                                    <input type="password" class="form-control" name="new_userRRN"
                                        placeholder="RR Number" pattern="[0-9]{13}" required="required">
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-phone"></i></span>
                                    <input type="tel" class="form-control" name="new_userPN"
                                        placeholder="Phone Number" pattern="[0-9]{11}" required="required">
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="input-group">
                                    <span class="input-group-addon"><i class="fa fa-envelope"></i></span>
                                    <input type="text" class="form-control" name="new_userEmail" placeholder="E-mail"
                                        required="required">
                                </div>
                            </div>
                            <div class="form-group">
                                <button type="submit" id="submit_button"
                                    class="btn btn-primary btn-block btn-lg">회원가입</button>
                            </div>
                            <script>
                                $("#submit_button").click(function () {
                                    var new_userID = $("input[name=new_userID]").val();
                                    var new_userPW = $("input[name=new_userPW]").val();
                                    var new_userPW_confirm = $("input[name=new_userPW_confirm]").val();
                                    var new_userName = $("input[name=new_userName]").val();
                                    var new_userRRN = $("input[name=new_userRRN]").val();
                                    var new_userPN = $("input[name=new_userPN]").val();
                                    var new_userEmail = $("input[name=new_userEmail]").val();

                                    //var params = [new_userID, new_userPW, new_userPW_confirm, new_userName, new_userPN, new_userEmail];

                                    $.post('/join',
                                        {
                                            newid: new_userID,
                                            newpw: new_userPW,
                                            newpw_c: new_userPW_confirm,
                                            newusername: new_userName,
                                            newuserRRN: new_userRRN,
                                            newuserPN: new_userPN,
                                            newuserEmail: new_userEmail
                                        }, function (data) {
                                            if (data.msg != null) {
                                                alert(data.msg);
                                                if (data.msg == "회원가입 성공!")
                                                    window.location.reload();
                                            }
                                            else {
                                                alert("alert");
                                            }
                                        })
                                });
                            </script>
                        </form>
                    </div>
                    <div class="modal-footer"></div>
                </div>
            </div>
        </div>

        <div>
            <%- body %>
        </div>


